{
  "containerDefinitions": [
    {
      "command": [
        "bundle",
        "exec",
        "rails",
        "s",
        "-p",
        "3000",
        "-b",
        "0.0.0.0"
      ],
      "cpu": 333,
      "environment": [
        {
          "name": "RAILS_MASTER_KEY",
          "value": "f3b59b39be91f577e6d279400d5a6f46"
        },
        {
          "name": "RAILS_ENV",
          "value": "production"
        }
      ],
      "essential": true,
      "image": "231200130686.dkr.ecr.ap-northeast-1.amazonaws.com/sample-image:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "sample-service",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "service"
        }
      },
      "memoryReservation": 600,
      "mountPoints": [],
      "name": "sample-webapp",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "DB_NAME",
          "valueFrom": "/sample/DB_NAME"
        },
        {
          "name": "DB_USER",
          "valueFrom": "/sample/DB_USER"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "/sample/DB_PASSWORD"
        },
        {
          "name": "DB_PORT",
          "valueFrom": "/sample/DB_PORT"
        },
        {
          "name": "DB_HOST",
          "valueFrom": "/sample/DB_HOST"
        }
      ],
      "volumesFrom": []
    }
  ],
  "family": "sample-service",
  "networkMode": "bridge",
  "placementConstraints": [],
  "taskRoleArn": "arn:aws:iam::231200130686:role/sample-ecs-task-role",
  "executionRoleArn": "arn:aws:iam::231200130686:role/sample-ecs-task-role",
  "volumes": []
}
